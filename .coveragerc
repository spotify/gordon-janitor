[run]
branch = True
source =
    gordon_janitor
omit = gordon_janitor/interfaces.py

[paths]
source =
   gordon_janitor
   .tox/*/lib/python*/site-packages/gordon_janitor
   .tox/pypy/site-packages/gordon_janitor

[report]
show_missing = True
exclude_lines =
    pragma: no cover
    if __name__ == .__main__.:
